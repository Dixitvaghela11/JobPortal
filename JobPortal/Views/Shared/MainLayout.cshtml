                                <div class="main-menu">
                                    <nav class="d-none d-lg-block">
                                        <ul id="navigation">
                                            <li><a href="@Url.Content("~/Home/Index")">Home</a></li>
                                            <li><a href="@Url.Content("~/Home/Jobs")">Find a Jobs </a></li>
                                            <li><a href="@Url.Content("~/Home/About")">About</a></li>

                                            <li><a href="@Url.Content("~/Home/ContactUs")">Contact</a></li>
                                        </ul>
                                    </nav>
                                </div>
                                <!-- Header-btn -->
                                <div class="header-btn d-none f-right d-lg-block">
                                    <a href="@Url.Content("~/Home/JobSeekerRegister")" class="btn head-btn1">Register</a>
                                    <a href="@Url.Content("~/Home/Login")" class="btn head-btn2">Login</a>
                                </div>
                            </div>
                        </div>
                        <!-- Mobile Menu -->
                        <div class="col-12">
                            <div class="mobile_menu d-block d-lg-none"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Header End -->
    </header>
    @{
        string message = TempData["Message"] as string;
        if (!String.IsNullOrEmpty(message))
        {
            <div class="alert alert-success alert-dismissible fade show" role="alert">
                <strong> @message</strong>
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <script>
                function preventBack() {
                    window.history.forward();
                }

                setTimeout(preventBack, 0);

                window.onunload = function () {
                    null;
                };
            </script>
        }
    }
